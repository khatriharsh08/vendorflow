# =========================================================
# VendorFlow .gitignore (Laravel + React/Vite)
# =========================================================

# Dependencies
/vendor/
/node_modules/
auth.json

# Environment files and local secrets
.env
.env.*
!.env.example

# Build artifacts
/public/build/
/public/hot
/public/storage
/public/mix-manifest.json
.vite/

# Laravel runtime files
/bootstrap/cache/*
