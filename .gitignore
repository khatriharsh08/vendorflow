/coverage/
/.phpunit.cache/
.phpunit.result.cache
.php-cs-fixer.cache
.phpactor.json
.phpstan.cache/
.pest/
clover.xml
junit.xml
phpunit.xml
!phpunit.xml.dist
/tests/Browser/screenshots/
/tests/Browser/console/
.nyc_output/
coverage-final.json

